<%- include('partials/header'); -%>

<h1>Home</h1>
<h2>/compose for access to sharing post.</h2>
<p> <%= startingContent %> </p>
<hr class="start">

<%  posts.forEach(function(post){ %>
    <h1><%= post.title %></h1>
    <p><%= post.content.substring(0, 100) + '...'%>
    <a href="/posts/<%=post.title%>">Read More</a></p>
    <hr>
<%  });                              %>


<%- include('partials/footer'); -%>
